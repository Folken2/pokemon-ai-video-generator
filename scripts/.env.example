# Gemini API Configuration
# Get your API key from: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter API Configuration
# Get your API key from: https://openrouter.ai/keys
OPENROUTER_API_KEY=your_openrouter_api_key_here

# KIE.ai (Kling 2.5 Pro) API Configuration
KIE_API_KEY=your_kie_api_key_here

# ElevenLabs API Configuration
ELEVENLABS_API_KEY=your_elevenlabs_api_key_here
ELEVENLABS_VOICE_ID=your_elevenlabs_voice_id_here

# Telegram Bot (Optional — for mobile approvals)
# Create a bot via @BotFather, get chat ID via @userinfobot
TELEGRAM_ENABLED=false
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# API Key Authentication (Optional — for cloud deployment)
# When set, all /api/ and /files/ routes require X-API-Key header.
# Leave empty for local development (no auth).
API_KEY=
